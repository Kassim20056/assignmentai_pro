# Free AI Configuration
# Using Hugging Face Inference API (free tier available)
VITE_AI_PROVIDER=huggingface
VITE_HUGGINGFACE_API_URL=https://api-inference.huggingface.co/models
VITE_HUGGINGFACE_MODEL=microsoft/DialoGPT-medium
VITE_HUGGINGFACE_API_KEY=your_huggingface_token_here

# Alternative: Use local AI (no API key needed)
# VITE_AI_PROVIDER=local
# VITE_LOCAL_AI_URL=http://localhost:11434

# Backup: Use mock AI responses for development
VITE_AI_PROVIDER=mock
VITE_ENABLE_MOCK_AI=true